{
  "extends": ["next/core-web-vitals", "prettier", "eslint:recommended"],
  "parser": "@typescript-eslint/parser",
  "env": {
    "jest": true,
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "warnOnUnsupportedTypeScriptVersion": false
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "warn",
    "no-var": "warn",
    "no-unused-vars": "off",
    "no-redeclare": "off"
  }
}
